# Environment Variables for Open Notebook

# Database Configuration (SurrealDB)
SURREAL_ADDRESS=127.0.0.1
SURREAL_PORT=8000
SURREAL_USER=root
SURREAL_PASS=root
SURREAL_NAMESPACE=open_notebook
SURREAL_DATABASE=open_notebook

# AI Model Provider API Keys
# Choose the providers you want to use and add their API keys

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google (Gemini)
GOOGLE_API_KEY=your_google_api_key_here

# Mistral
MISTRAL_API_KEY=your_mistral_api_key_here

# DeepSeek
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# xAI (Grok)
XAI_API_KEY=your_xai_api_key_here

# Groq
GROQ_API_KEY=your_groq_api_key_here

# Ollama (for local models)
OLLAMA_API_BASE=http://localhost:11434

# ElevenLabs (for text-to-speech)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Voyage AI (for embeddings)
VOYAGE_API_KEY=your_voyage_api_key_here

# Azure OpenAI
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here
AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name_here
AZURE_OPENAI_API_VERSION=2024-02-01

# Google Vertex AI
VERTEX_PROJECT=your_gcp_project_id
VERTEX_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=./google-credentials.json

# OpenRouter
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Content Processing Services
# Firecrawl (for web scraping)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Jina AI (for web scraping)
JINA_API_KEY=your_jina_api_key_here
